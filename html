		function onhover(obj){
			obj.className='current';
		}
		function outhover(obj){
			obj.className='';
		}

		(function(){
			new FixedLayer('baner');
		})();

		(function(){
			var change_tab = document.getElementById('change_tab'),
				lis =change_tab.getElementsByTagName('li'),
				allnet= document.getElementById('allnet'),
				youku = document.getElementById('youku'),
				tudou = document.getElementById('tudou'),
				anchors = $('#youanchor');
				anchors.hide();
			var url = window.location.href;//通过这种方式获取的url可能为空
			var w = url.indexOf("?type=");
			url = url.substr(0 , w);
			//alert("xxx:"+url);
			if(url == "" || url == null){
				url = document.URL;
			}

			//获取节目id
			var sids = "";
			sids = getQueryString("sids");

			if(lis){
				lis[0].onclick = function(){
					//埋点
					track(4008608);
					if(sids != null && sids !=""){
						window.location.href= url + '?type=alldata&sids='+sids;
					}else{
						window.location.href= url + '?type=alldata';
					}

				}
				lis[1].onclick = function(){
					//埋点
					track(4008609);
					if(sids != null && sids !=""){
						window.location.href= url + '?type=youku&sids='+sids;
					}else{
						window.location.href= url + '?type=youku';
					}
				}
