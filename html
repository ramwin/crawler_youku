						var app = VRCHART.Panel$pie$.show('youkucrowd', data2);
					});

					VRCHART.loadChartJS('pie', function(){
						var app = VRCHART.Panel$pie$.show('device', data4);
					});

					VRCHART.loadChartJS('pie', function(){
						if(data5[0].domain == "其他"){
							data5 = eval('('+'[{"platform":[],"domain":[]}]'+')');
						}
						var app = VRCHART.Panel$pie$.show('platform', data5);
					})


					VRCHART.loadChartJS('map', function(){
						var app = VRCHART.Panel$map$.show('youkuarea', data3);
						//获取地区top
						var cityTop = getTopCity(data3);
						var prefix = getPrefix();
						var html = "";
						$.each(cityTop,function(i,n){
							var strs = new Array();
							var city = cityTop[i].city;
							strs = (city+"").split(":");
							//alert(getCityName(strs[0]));

							var strs1 = new Array();
							var province = cityTop[i].province;
							strs1 = (province+"").split(":");
							//alert(getProvinceName(strs1[0]));
							var tag = "";
							if(i == 0){
								style = "current";
							}else if(i == 1){
								style = "";
							}else{
								style = "last";
							}

							var provinceName = "";
							if(typeof(getProvinceName(strs1[0])) == "undefined"){
								provinceName = "暂无数据";
							}else{
								provinceName = getProvinceName(strs1[0]);
							}
							var cityName = "";
							if(typeof(getCityName(strs[0])) == "undefined"){
								cityName = "暂无数据";
							}else{
