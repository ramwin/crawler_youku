						//获取地区top
						var cityTop = getTopCity(tudoudata3);
						var prefix = getPrefix();
						var html = "";
						$.each(cityTop,function(i,n){
							var strs = new Array();
							var city = cityTop[i].city;
							strs = (city+"").split(":");
							//alert(getCityName(strs[0]));

							var strs1 = new Array();
							var province = cityTop[i].province;
							strs1 = (province+"").split(":");
							//alert(getProvinceName(strs1[0]));
							var tag = "";
							if(i == 0){
								tag = "A";
								style = "current";
							}else if(i == 1){
								tag = "B";
								style = "";
							}else{
								tag = "C";
								style = "last";
							}

							var provinceName = "";
							if(typeof(getProvinceName(strs1[0])) == "undefined"){
								provinceName = "暂无数据";
							}else{
								provinceName = getProvinceName(strs1[0]);
							}
							var cityName = "";
							if(typeof(getCityName(strs[0])) == "undefined"){
								cityName = "暂无数据";
							}else{
								cityName = getCityName(strs[0]);
							}

							if(cityName == "暂无数据" || provinceName == "暂无数据"){
								html = '<div class="field_info '+style+'" onclick="track(4008625);changeCity('+i+');">'+
											'<div class="fieldbox">'+
									        	'<div class="nodata">暂无数据</div>'+
									        '</div>'+
									    '</div>';
							}else{
								html = '<div class="field_info '+style+'" onclick="track(4008625);changeCity('+i+');">'+
											'<div class="fieldbox">'+
									        	'<div class="fieldname">'+provinceName+'</div>'+
									            '<div class="subname">最高省份</div>'+
									        	'<div class="fieldname">'+cityName+'</div>'+
									            '<div class="subname">最高城市</div>'+
									        '</div>'+
									    '</div>';
							}
							 $("#"+prefix+"field").append(html);
							 if(i < cityTop.length - 1 ){
								 $("#"+prefix+"field").append('<div class="clear"></div>');
							 }

							//地区hover事件绑定
								$(".field_info").hover(
										  function () {
										    $(this).addClass("hover");
										  },
										  function () {
										    $(this).removeClass("hover");
										  }
										);
						});
